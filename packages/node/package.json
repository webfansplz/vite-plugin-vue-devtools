{
  "name": "vite-plugin-vue-devtools",
  "type": "module",
  "version": "1.0.0-rc.5",
  "description": "A vite plugin for Vue DevTools",
  "author": "webfansplz",
  "license": "MIT",
  "homepage": "https://github.com/webfansplz/vite-plugin-vue-devtools#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webfansplz/vite-plugin-vue-devtools.git",
    "directory": "packages/node"
  },
  "bugs": {
    "url": "https://github.com/webfansplz/vite-plugin-vue-devtools/issues"
  },
  "keywords": [
    "vue-devtools",
    "vite-plugin",
    "vite-plugin-vue-devtools",
    "dx"
  ],
  "exports": {
    ".": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.cjs"
    },
    "./*": "./*",
    "./client": {
      "types": "./client.d.ts"
    }
  },
  "main": "dist/vite.cjs",
  "module": "dist/vite.mjs",
  "types": "dist/vite.d.ts",
  "files": [
    "dist",
    "./src/views/*",
    "./src/app.js",
    "*.d.ts"
  ],
  "engines": {
    "node": ">=v14.21.3"
  },
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub"
  },
  "peerDependencies": {
    "vite": "^3.1.0 || ^4.0.0-0"
  },
  "dependencies": {
    "@vite-plugin-vue-devtools/core": "workspace:*",
    "@webfansplz/vuedoc-parser": "^0.0.4",
    "birpc": "^0.2.14",
    "execa": "^8.0.1",
    "sirv": "^2.0.3",
    "vite-plugin-inspect": "^0.7.40",
    "vite-plugin-vue-inspector": "^3.7.2"
  },
  "devDependencies": {
    "@types/node": "^20.8.3",
    "fast-glob": "^3.3.1",
    "image-meta": "^0.1.1",
    "pathe": "^1.1.1"
  }
}
