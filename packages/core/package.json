{
  "name": "@vite-plugin-vue-devtools/core",
  "type": "module",
  "version": "1.0.0-beta.0",
  "author": "webfansplz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webfansplz/vite-plugin-vue-devtools.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
