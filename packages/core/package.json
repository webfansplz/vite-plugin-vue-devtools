      "types": "./types.d.ts",
      "require": "./dist/types.cjs",
      "import": "./dist/types.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts",
    "*.cjs",
    "*.mjs"
