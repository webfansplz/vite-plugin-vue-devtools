  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "vite": "^3.1.0 || ^4.0.0-0"
  },
  "dependencies": {
    "birpc": "^0.2.12",
    "vite-hot-client": "^0.2.1"
  },
  "devDependencies": {
    "unbuild": "^1.2.1"
  }
