{
  "name": "vite-plugin-vue-devtools",
  "version": "0.0.16",
  "packageManager": "pnpm@8.4.0",
  "description": "A vite plugin for Vue Devtools",
  "author": "webfansplz",
  "license": "MIT",
  "homepage": "https://github.com/webfansplz/vite-plugin-vue-devtools#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webfansplz/vite-plugin-vue-devtools.git"
  },
  "bugs": {
    "url": "https://github.com/webfansplz/vite-plugin-vue-devtools/issues"
  },
  "keywords": [
    "vue-devtools",
    "vite-plugin",
    "vite-plugin-vue-devtools",
    "dx"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src/node/app.js",
    "src/node/Container.vue",
    "*.d.ts"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "rimraf dist && run-s build:*",
    "build:client": "vite build src/client",
    "build:js": "unbuild",
    "dev": "nr stub && vite src/client",
    "dev:client": "vite build src/client --watch",
    "play": "nr -C playground dev",
    "stub": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish"
  },
  "peerDependencies": {
    "vite": "^3.0.0-0 || ^4.0.0-0"
  },
  "dependencies": {
    "@vueuse/core": "^10.1.2",
    "nanoid": "^4.0.2",
    "sirv": "^2.0.3",
    "splitpanes": "^3.1.5",
    "vite-hot-client": "^0.2.1",
    "vite-plugin-inspect": "^0.7.26",
    "vite-plugin-vue-inspector": "^3.4.1",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.5",
    "@antfu/ni": "^0.21.3",
    "@iconify/json": "^2.2.61",
    "@nuxt/devtools-ui-kit": "^0.4.5",
    "@types/codemirror": "^5.60.7",
    "@types/node": "^20.1.0",
    "@types/splitpanes": "^2.2.1",
    "@unocss/eslint-config": "^0.51.12",
    "@unocss/reset": "^0.51.12",
    "@vitejs/plugin-vue": "^4.2.1",
    "bumpp": "^9.1.0",
    "codemirror": "^6.0.1",
    "codemirror-theme-vars": "^0.1.2",
    "dayjs": "^1.11.7",
    "eslint": "8.38.0",
    "fast-glob": "^3.2.12",
    "floating-vue": "2.0.0-beta.20",
    "fuse.js": "^6.6.2",
    "image-meta": "^0.1.1",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "ohash": "^1.1.2",
    "pathe": "^1.1.0",
    "pinia": "^2.0.35",
    "pnpm": "^8.4.0",
    "rimraf": "^5.0.0",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.1",
    "unocss": "^0.51.12",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "vis-data": "^7.1.6",
    "vis-network": "^9.1.6",
    "vite": "^4.3.5",
    "vite-dev-rpc": "^0.1.2",
    "vite-plugin-pages": "^0.29.0",
    "vue": "^3.2.47",
    "vue-virtual-scroller": "2.0.0-beta.8"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
